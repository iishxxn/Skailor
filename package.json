{
  "name": "skailor",
  "version": "1.0.0",
  "private": true,
  "description": "Skailor: An AI-powered education platform built with a modern stack.",
  "workspaces": [
  "apps/frontend",
  "apps/backend"
],
  "scripts": {
  "start": "npm --workspace=backend start",
  "build": "npm run build --workspace=frontend && npm run build --workspace=backend",
  "build:frontend": "npm run build --workspace=frontend",
  "build:backend": "npm run build --workspace=backend",
  "dev": "concurrently \"npm run dev:frontend\" \"npm run start:backend\"",
  "dev:frontend": "npm run dev --workspace=frontend",
  "start:backend": "npm run start --workspace=backend",
  "lint": "npm run lint --workspace=frontend && npm run lint --workspace=backend",
  "format": "npm run format --workspace=frontend && npm run format --workspace=backend"
},
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "author": "Ishaan Kalia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IshaanKalia/Skailor.git"
  },
  "bugs": {
    "url": "https://github.com/IshaanKalia/Skailor/issues"
  },
  "homepage": "https://github.com/IshaanKalia/Skailor#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.10"
  }
}
